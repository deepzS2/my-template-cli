{
  "name": "detc",
  "version": "1.3.0",
  "description": "detc (deepz template cli) é um gerador de templates criado para falicitar criação de novos projetos e estudar sobre CLI no NodeJS com Typescript!",
  "main": "lib/index.js",
  "license": "MIT",
  "homepage": "https://github.com/deepzS2/my-template-cli#readme",
  "bugs": {
    "url": "https://github.com/deepzS2/my-template-cli/issues"
  },
  "author": {
    "name": "deepzS2",
    "url": "https://github.com/deepzS2/my-template-cli"
  },
  "keywords": [
    "detc",
    "deepz-template-cli"
  ],
  "bin": {
    "detc": "lib/index.js",
    "deepz-template-cli": "lib/index.js"
  },
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn lint && tsc && babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline && shx rm -rf lib/templates && shx cp -r src/templates lib/templates",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --config .eslintrc.json --fix --ext .ts src",
    "pretest": "yarn build",
    "test": "NODE_ENV=test jest",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/concat-stream": "^2.0.0",
    "@types/ejs": "^3.1.0",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.13",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-mock-process": "^1.4.1",
    "prettier": "^2.5.1",
    "shx": "^0.3.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^7.0.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "chalk": "4.1.2",
    "concat-stream": "^2.0.0",
    "ejs": "^3.1.6",
    "inquirer": "^8.2.0",
    "os-locale": "6.0.0",
    "shelljs": "^0.8.5",
    "yargs": "^17.3.1"
  }
}