{
  "name": "detc",
  "version": "1.0.2",
  "description": "detc (DEepz Template Cli) é um gerador de templates criado para falicitar criação de novos projetos e estudar sobre CLI no NodeJS com Typescript!",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://github.com/deepzS2/my-template-cli#readme",
  "bugs": {
    "url": "https://github.com/deepzS2/my-template-cli/issues"
  },
  "author": {
    "name": "deepzS2",
    "url": "https://github.com/deepzS2/my-template-cli"
  },
  "keywords": [
    "detc",
    "deepz-template-cli"
  ],
  "bin": {
    "detc": "dist/index.js",
    "deepz-template-cli": "dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn lint && tsc && shx rm -rf dist/templates && shx cp -r src/templates dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --config .eslintrc.json --fix --ext .ts .",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/ejs": "^3.1.0",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.13",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "shx": "^0.3.4",
    "ts-node": "^7.0.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "ejs": "^3.1.6",
    "inquirer": "^8.2.0",
    "shelljs": "^0.8.5",
    "yargs": "^17.3.1"
  }
}
